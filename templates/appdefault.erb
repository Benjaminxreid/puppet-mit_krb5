  <%= @app %> = {
  <%- @_settings.each do | key, value | -%>
    <%- if value.kind_of?(Array) -%>
       <%= "#{key}:" %>
       <% value.each do |v| -%>
                        <%= "- #{v}" %>
                        <% end -%>
       <%- else -%>
       <%= "#{key.chomp(':')} = #{value}" %>
       <%- end -%>
                <%- end -%>
    }
